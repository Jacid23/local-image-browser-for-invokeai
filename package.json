{
  "name": "local-image-browser-for-invokeai",
  "description": "Local image browser for browsing InvokeAI outputs (unaffiliated)",
  "author": "LuqP2 <github.com/LuqP2>",
  "private": true,
  "version": "1.7.6",
  "type": "module",
  "main": "electron.mjs",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron-pack": "npm run build && electron-builder",
    "electron-dist": "npm run build && electron-builder --publish=never",
    "release": "npm run build && electron-builder --publish=always",
    "generate-release": "node generate-release.js",
    "release-workflow": "node release-workflow.js",
    "auto-release": "node auto-release.js"
  },
  "dependencies": {
    "electron-updater": "^6.6.2",
    "exifr": "^7.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.5.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "wait-on": "^9.0.0"
  }
}
